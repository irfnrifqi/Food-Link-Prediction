file = {:C\:/Users/Irfan/OneDrive - Institut Teknologi Sepuluh Nopember/Kuliah/Semester 7/Pra TA/Referensi/Link Prediction in Social Networks The State Of The Art.pdf:pdf},
issn = {18691919},
journal = {Science China Information Sciences},
keywords = {dynamic network,learning model,link prediction,similarity metric,social network},
number = {1},
pages = {1--38},
title = {{Link prediction in social networks: the state-of-the-art}},
volume = {58},
year = {2015}
}
